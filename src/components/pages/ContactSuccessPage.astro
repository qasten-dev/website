---
import Layout from "@/layouts/Layout.astro";
import { getTranslations } from "@/utils/i18n";
import SuccessCard from "../SuccessCard.astro";

const t = getTranslations(Astro.currentLocale);
const { title, subtitle } = t.contactSuccess;
const description = title + " " + subtitle;
---

<Layout title="Contact" description={description}>
  <SuccessCard title={title} subtitle={subtitle} />
</Layout>
